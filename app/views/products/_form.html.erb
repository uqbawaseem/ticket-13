<%= form_for @product do |f|%>
    <%= f.label :Image%>
    <%= f.file_field :image%><br><br>
    <%= f.label :Name%>
    <%= f.text_field :name%><br><br>
    <%= f.label :came%>
    <%= f.text_field :color%><br><br>
    <%= f.label :Price%>
    <%= f.text_field :price%><br><br>
    <%= f.submit :"Add"%>
<% end %>